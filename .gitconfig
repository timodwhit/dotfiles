[user]
  name = Tim Whitney
  email = tim.d.whitney@gmail.com
[alias]
  co = checkout
  cob = checkout -b
  ci = commit
  st = status
  br = branch
  cp = cherry-pick
  diffc = diff --cached
  dc = diff -cached
  pud = pull upstream develop
  pum = pull upstream master
  put = "! git push -u tim `git rev-parse --abbrev-ref HEAD`"
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green
  changed = yellow
  untracked = red
[push]
  default = simple
